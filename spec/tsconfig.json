{
	"compilerOptions": {
		"module": "commonjs",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noEmitOnError": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"removeComments": true,
		"sourceMap": true,
		"target": "es6",
		"lib": ["es7", "dom"],
		"declaration": true
	}
}
